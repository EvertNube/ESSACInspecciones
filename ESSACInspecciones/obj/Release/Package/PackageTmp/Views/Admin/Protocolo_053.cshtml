                                                                                                         case 4:
                                                                                                        <td rowspan='@seccion.SeccionBodys[s].Rowspan' colspan='@seccion.SeccionBodys[s].Colspan'>
                                                                                                            @Html.DropDownListFor(u => u.GroupDescripcion, new SelectList(ViewBag.Items_SelectBomba, "id", "name", seccion.SeccionBodys[s].Respuesta != null ? Convert.ToInt32(seccion.SeccionBodys[s].Respuesta) : 0), new { @class = "input-sm parsley-validated", @style = "width: auto;" })
